{
  "name": "svelte-events",
  "description": "TODO",
  "author": "jackdbd <jackdebidda@gmail.com> (http://www.giacomodebidda.com/)",
  "homepage": "https://jackdbd.github.io/svelte-events",
  "repository": "git@github.com:jackdbd/svelte-events.git",
  "version": "0.1.0",
  "main": "./src/index.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.config.js --progress",
    "ci": "yarn lint && yarn test && yarn coverage && yarn build",
    "clean": "rimraf build",
    "coverage": "codecov",
    "deploy": "gh-pages -d build",
    "dev": "webpack-dev-server --content-base public --env.svelteDevMode=TRUE",
    "format": "eslint -c .eslintrc --fix --ext .svelte,.js --fix src",
    "format:prettier": "prettier --write src/**/{*.svelte,*.js}",
    "lint": "yarn lint:css && yarn lint:js",
    "lint:css": "stylelint --config .stylelintrc.json --formatter verbose './src/**/*.{css,svelte}'",
    "lint:js": "eslint -c .eslintrc --ext .svelte,.js src",
    "ncu": "ncu",
    "ncuu": "ncu --upgrade",
    "nuke": "rimraf node_modules && rm yarn.lock",
    "predeploy": "yarn clean && yarn build",
    "static": "python -m webbrowser ~/repos/svelte-events/public/index.html",
    "test": "jest --coverage --runInBand --verbose",
    "updtr": "updtr --use yarn --test-stdout",
    "webhint": "hint https://jackdbd.github.io/svelte-events",
    "webhint:report": "python -m webbrowser ~/repos/svelte-events/hint-report/https-jackdbd-github-io-svelte-events/index.html"
  },
  "dependencies": {
    "@babel/polyfill": "7.4.4",
    "babel-plugin-inline-dotenv": "1.3.3",
    "es6-promise": "4.2.8",
    "isomorphic-fetch": "2.2.1",
    "query-string": "5.1.1",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/plugin-transform-strict-mode": "7.2.0",
    "@babel/preset-env": "7.5.5",
    "@babel/register": "7.5.5",
    "@testing-library/jest-dom": "4.1.2",
    "@testing-library/svelte": "1.8.0",
    "autoprefixer": "9.6.1",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-add-module-exports": "1.0.2",
    "cross-env": "5.2.0",
    "css-loader": "2.1.1",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-svelte3": "2.7.3",
    "file-loader": "4.1.0",
    "gh-pages": "2.1.1",
    "hint": "5.4.1",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.8.0",
    "jest-transform-svelte": "2.0.2",
    "mini-css-extract-plugin": "0.6.0",
    "nock": "10.0.6",
    "node-sass": "4.12.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.18.2",
    "prettier-plugin-svelte": "0.7.0",
    "serve": "11.0.0",
    "style-loader": "0.23.1",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "19.0.0",
    "svelte": "3.12.1",
    "svelte-loader": "2.13.3",
    "svelte-preprocess": "2.15.0",
    "updtr": "^3.1.0",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.3.1"
  },
  "keywords": [
    "svelte",
    "webpack"
  ]
}
